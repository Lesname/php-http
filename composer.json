{
  "name": "lessname/http",
  "description": "Http helpers",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json"
    }
  ],
  "require": {
    "php": "^8.2.0",

    "ext-intl": "*",

    "lessname/validator": "^0.3.0",
    "lessname/cache": "^0.2.0",
    "lessname/database": "^0.2.0",

    "psr/http-server-middleware": "^1.0.1",
    "psr/http-factory": "^1.0.1",

    "symfony/translation": "^6.2.0 || ^7.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^1.4.0",
    "phpunit/phpunit": "^10.0.0",
    "vimeo/psalm": "^5.0.0",

    "lessname/token": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "LessHttp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessHttpTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
