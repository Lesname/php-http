{
  "name": "lesname/http",
  "license": "MIT",
  "require": {
    "php": "^8.4.0",
    "ext-ctype": "*",

    "lesname/validator": "^0.6.0",
    "lesname/cache": "^0.4.0",
    "lesname/database": "^0.5.0",

    "psr/http-server-middleware": "^1.0.0",
    "psr/http-factory": "^1.0.0",

    "symfony/translation": "^7.0.0 || ^8.0.0"
  },
  "require-dev": {
    "ext-intl": "*",

    "lesname/token": "^0.4.0",

    "squizlabs/php_codesniffer": "^4.0.0",
    "phpunit/phpunit": "^12.0.0",

    "phpstan/phpstan": "^2.0.0",
    "phpstan/extension-installer": "^1.4.0",
    "phpstan/phpstan-deprecation-rules": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "LesHttp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LesHttpTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
